ul.tabjs {
	margin: 0 0 1px 0;
	padding: 0;
	float: left;
	list-style: none;
	height: 31px; /*--Set height of tabs--*/
	width: 100%;
}
ul.tabjs li {
	float: left;
	margin: 0 3px 0 0;
	padding: 0;
	height: 31px; /*--Subtract 1px from the height of the unordered list--*/
	line-height: 31px; /*--Vertically aligns the text within the tab--*/
	overflow: hidden;
	position: relative;
	background: #e0e0e0;
	-moz-border-radius:7px 7px 0 0;
	-webkit-border-radius:7px 7px 0 0;
	border-radius:7px 7px 0 0;
	border:solid 1px #005f96;
	border-bottom:none;
	-moz-box-shadow:2px -2px 5px 0px #CBCBCB;
	-webkit-box-shadow:2px -2px 5px 0px #CBCBCB;
	box-shadow:2px -2px 5px 0px #CBCBCB;
}
ul.tabjs li a {
	text-decoration: none;
	color: #000;
	display: block;
	padding: 0 20px;
	border: 1px solid #fff; /*--Gives the bevel look with a 1px white border inside the list item--*/
	outline: none;
	border-bottom:none;
	-moz-border-radius:7px 7px 0 0;
	-webkit-border-radius:7px 7px 0 0;
	border-radius:7px 7px 0 0;
	font-weight:bold;
}
ul.tabjs li a:hover {
	background: #ccc;
}
html ul.tabjs li.active, html ul.tabjs li.active a:hover  { /*--Makes sure that the active tab does not listen to the hover properties--*/
	background: #005f96;/*--Makes the active tab look like it's connected with its content--*/
	-moz-border-radius:7px 7px 0 0;
	-moz-box-shadow:2px -2px 5px 0px #CBCBCB;
}
html ul.tabjs li.active a{
	color:#fff;
	border: 1px solid #3985b0;
}
.tab_container {
	overflow: hidden;
	clear: both;
	float: left; width: 100%;
	border:solid 1px #005f96;
	border-top:solid 3px #005f96;
	padding-bottom:30px;
	-moz-box-shadow:1px 2px 6px 3px #CBCBCB;
	-webkit-box-shadow:1px 2px 6px 3px #CBCBCB;
	box-shadow:1px 2px 6px 3px #CBCBCB;
	font-family:Arial, Helvetica, sans-serif;

}
