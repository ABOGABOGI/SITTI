.f20{
	font-size:20px;
}
.headerZen{
	height:80px;
	padding:90px 10px 40px 10px;
}
a.logoZen{
	/*background:url(../images/logo_zen.png) no-repeat;
	background:url(../images/logo_zen.png) no-repeat;
