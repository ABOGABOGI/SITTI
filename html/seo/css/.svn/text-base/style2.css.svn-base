
/* CSS Document */


#page{
	margin: 0 auto;
	width:1000px;
	text-align: left;
	background-image:url(../images/css/background.png);
}
#header{

	background-image:url(../images/css/but_head.png);
}
#menu	{
	font-family: Arial, sans-serif;
	font-weight: bold;
	text-transform: uppercase
	margin: 50px 0;
	padding: 9px 76px;
	list-style-type: none;
	font-size: 13px;
	height: 50px;

}
#menu li {
	float: left;
	margin: 0;
}

#menu li a {
	text-decoration: none;
	display: block;
	padding: 0 10px;
	line-height: 40px;
	color	:#666;
}
#menu_wrapper li a {
	color: #666060;
}
#menu_wrapper li a:hover {
	color: #999;
}

#content_seo{
  float: left;
    margin-left: 1px;
