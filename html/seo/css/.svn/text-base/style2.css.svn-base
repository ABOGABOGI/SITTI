	width:996px;
	height:332px;
	background-image:url(../images/css/banner.png);
	background-repeat: no-repeat;
}

#hadimg{
	
	width:996px;
	height:411px;
	background-image:url(../images/css/hadiah.png);
	background-repeat: no-repeat;
}

#contentinf{
	width:455px;
	float: right;
}
#inf{
}
#inf p a.situsmu {
	display: block;
	width: 255px; height: 45px;
	background-image:url(../images/css/stickButton.png);
	text-indent: -9999px;
	background-repeat:no-repeat;
	margin-right: 110px;
    margin-top: 80px;
	float:right;
}

#inf p a.situsmu:hover {
	display: block;
	width: 255px; height: 45px;
	background-image:url(../images/css/stickButtonHover.png);
	text-indent: -9999px;
	background-repeat:no-repeat;
	margin-right: 110px;
    margin-top: 80px;
	float:right;
}

