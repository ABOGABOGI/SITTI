}
.try-us .bar #logo .loader {
	margin-bottom:20px;
}

/* FORM
------------------------------- */

form#contact .bar {
	height:185px;
	overflow:hidden;
	position:relative;
}
form#contact .blk {
	margin-right:10px;
	position:absolute;
	top:0; left:0; right:0; bottom:0;
}
