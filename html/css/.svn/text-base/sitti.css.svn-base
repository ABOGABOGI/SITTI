/*Gradient*/
.gradient{
	background-image: linear-gradient(right , rgb(0,145,255) 1%, rgb(250,0,13) 75%);
	background-image: -o-linear-gradient(right , rgb(0,145,255) 1%, rgb(250,0,13) 75%);
	background-image: -moz-linear-gradient(right , rgb(0,145,255) 1%, rgb(250,0,13) 75%);
	background-image: -webkit-linear-gradient(right , rgb(0,145,255) 1%, rgb(250,0,13) 75%);
	background-image: -ms-linear-gradient(right , rgb(0,145,255) 1%, rgb(250,0,13) 75%);
	
	background-image: -webkit-gradient(
		linear,
		right top,
		left top,
		color-stop(0.01, rgb(0,145,255)),
		color-stop(0.75, rgb(250,0,13))
	);
		/* For Internet Explorer 5.5 - 7 */
        filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#cccccc, endColorstr=#ffffff);
        /* For Internet Explorer 8 */
        -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#cccccc, endColorstr=#ffffff)";    
        background-color: #CCC;
}

