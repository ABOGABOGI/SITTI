<script>
var key = "";
$(document).ready(function(){
	function DayDiff(date1, date2) {
		return (date2.getTime() - date1.getTime()) / 1000 / 60 / 60 / 24;
	}
	
	$("#tanggalAwal,#tanggalAkhir").datepicker();

    $('#tanggalAwal').bind('change', function () {
        $('#tanggalAkhir').val('');     
    });       
       
    $('#tanggalAkhir').bind('change', function () {
        var tgl_awal = $('#tanggalAwal').datepicker("getDate");
        var tgl_akhir = $('#tanggalAkhir').datepicker("getDate");
        interval = DayDiff(tgl_awal, tgl_akhir);
        
        if (interval<=0){
			$('#tanggalAkhir').val('');
		}
    });
	
	$('#product').bind('change', function () {
		$("#lbl_key_usulan").hide();
		$("#keyword_usulan").html("");
		if ($("#product").val()!=""){
			$("#keyword_usulan").html($("#product").val());
			if (key!=""){
				$("#keyword_usulan").html($("#keyword_usulan").html()+",");
			}
		}
		$("#keyword_usulan").html($("#keyword_usulan").html()+key);
    });
});
</script>
