            <script>
			//disable form
			function fdisable()
			{
			var get= $("#category option:selected").val();
			if(get == 0)
			{
			$("#keyword_usulan").attr("disabled", "true");
			$('.caution').show();
			$("#keyword_usulan").html($("#product").val());
			}
			else
			{
				data=$('#jenis').val();
				var len=data.length;
				if(len<1)
				{
				$('.jenisproduk2','.kategoriIklan').next().show();
				$('#keyword_usulan').attr('disabled',true);
				$('.caution').show();
				$("#keyword_usulan").html($("#product").val());
				}
				else
				{
				$('.jenisproduk2','.kategoriIklan').next().hide();
				$('#keyword_usulan').removeAttr('disabled');
				$('.caution').hide();
				
				$.get('buat.php',{get_key:1, cat:get, type:data}, function(data){
					key = data;
					$("#lbl_key_usulan").hide();
					$("#keyword_usulan").html("");
					if ($("#product").val()!=""){
						$("#keyword_usulan").html($("#product").val()+",");
					}
					$("#keyword_usulan").html($("#keyword_usulan").html()+data);
				});
				}
			
			}
			}
			</script>
           
