<script language="javascript">


var bpos = "";
var perc = 0 ;
var minperc = 0 ;

$('#email').keyup(function(){
	$('#emailCheck').html(emailValidate($('#email').val()));
	
    });
$('#namaLengkap').keyup(function() {
	$('#namaCheck').html(namaValidate($('#namaLengkap').val()));
	});
              
$('#password').css( {backgroundPosition: "0 0"} );

$('#password').keyup(function(){
	//alert($('#password').val()+","+$('#email').val());
	$('#result').html(passwordStrength($('#password').val())) ; 
	perc = passwordStrengthPercent($('#password').val());
	
	bpos=" $('#colorbar').css( {backgroundPosition: \"0px -" ;
	bpos = bpos + perc + "px";
	bpos = bpos + "\" } );";
	bpos=bpos +" $('#colorbar').css( {width: \"" ;
	bpos = bpos + (perc * 2) + "px";
	bpos = bpos + "\" } );";
	
	eval(bpos);
    $('#percent').html(" " + perc  + "% ");
    if(perc>0){
    	$('#percent').show();
    }
    
});
function validateForm()
{
var nama = $("#namaLengkap").val();
var pass = $("#password").val();
var email = $("#email").val();
var web = $("#website").val();
//alert(web);
if (nama==null || nama=="")
  	{
		$("#namaLengkap").focus();
  		return false;
  	}else if(email==null || email==""){
		$("#email").focus();
		return false;	
	}else if(pass==null || pass==""){
		$("#password").focus();
		return false;	
	}else if(web==null || web=="" || web=="http://"){
		//alert("tes");
		$("#website").focus();
		return false;	
	}
}	


</script>
